    <servlet-name>Search</servlet-name>
    <url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllUser</servlet-name>
    <url-pattern>/AllUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AllFriends</servlet-name>
    <url-pattern>/AllFriends</url-pattern>
  </servlet-mapping>
