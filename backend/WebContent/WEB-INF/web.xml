  </servlet>
  <servlet>
    <servlet-name>AllUser</servlet-name>
    <servlet-class>servlet.AllUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AllFriends</servlet-name>
    <servlet-class>servlet.AllFriends</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AllComment</servlet-name>
    <servlet-class>servlet.AllComment</servlet-class>
  </servlet>
