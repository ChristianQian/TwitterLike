Create table sample:
CREATE TABLE `sample` (
  `Uid` char(38) NOT NULL,
  PRIMARY KEY (`Uid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

Insert random into table sample:
INSERT INTO Sample(UId)
VALUES(UUID());
SELECT * FROM Sample;
